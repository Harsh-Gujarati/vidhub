{
    "rewrites": [
        {
            "source": "/api/mega/resolve",
            "destination": "/api/mega-resolve"
        },
        {
            "source": "/api/mega-stream/:path*",
            "destination": "/api/mega-stream"
        },
        {
            "source": "/api/terabox/resolve",
            "destination": "/api/terabox-resolve"
        },
        {
            "source": "/api/:path*",
            "destination": "/api/proxy"
        }
    ],
    "headers": [
        {
            "source": "/api/:path*",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, POST, PUT, DELETE, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "X-Requested-With, Content-Type, Accept"
                }
            ]
        }
    ]
}